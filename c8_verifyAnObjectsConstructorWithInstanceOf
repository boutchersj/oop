/* jshint expr: true */

function House(numBedrooms) {
  this.numBedrooms = numBedrooms;
}

// Add your code below this line

let myHouse = new House(1);

myHouse instanceof House;

//The 'instanceof' operator lets you verify if an object is an instance of a constructor
//If it is, it returns true, and false if not
//Objects identical to the constructor's object blueprint will return false if they are not made using the constructor
